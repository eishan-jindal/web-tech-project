    <li class="w3-right"><a href="dbms.html" class="w3-hover-white w3-hover-text-blue">Database</a></li>
    <!--Dropdown Menu-->
    <li class="w3-dropdown-hover w3-right"><a href="#" class="w3-hover-white w3-hover-text-blue">Works <i class="fa fa-caret-down"></i></a>
      <div class="w3-dropdown-content w3-card-2">
        <a href="#" class="w3-hover-white w3-hover-text-blue">Technical</a> <!--Department work-->
        <a href="#" class="w3-hover-white w3-hover-text-blue">GLL</a> <!--Project work-->
      </div>
    </li>
    <li class="w3-right"><a href = "#" class = "w3-hover-white w3-hover-text-blue"><i class="fa fa-cloud"></i> Drive</a></li>
    <li class="w3-right"><a href = "#" class= "w3-hover-white w3-hover-text-blue"><i class="fa fa-users"></i> Social Media</a></li>
    <li class="w3-right"><a href = "http://www.gmail.com" class="w3-hover-white w3-hover-text-blue"><i class="fa fa-envelope"></i> Gmail</a></li>
